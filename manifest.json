{
    "manifest_version": 2,
  
    "name": "Learn Re",
    "description": "Learn Re",
    "version": "0.1",
  
    "browser_action": {
      "default_popup": "src/popup.html"
    },
  
    "permissions": [
      "activeTab",
      "tabs"
    ],
  
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["src/build/main.js"]
      }
    ]
  }